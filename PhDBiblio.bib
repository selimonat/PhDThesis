	Doi = {10.1016/j.brainres.2007.09.045},
	Journal = {Brain Res},
	Keywords = {Male, Evoked Potentials, Acoustic Stimulation, Repression, Photic Stimulation, Auditory Perception, Humans, Adult, Female, Illusions, Psychophysics, Brain Mapping, Cerebral Cortex, Time Factors, Analysis of Variance, visual perception, Reaction Time, Nonlinear Dynamics},
	Language = {eng},
	Month = {Jul},
	Pages = {132--41},
	Pii = {S0006-8993(07)02235-4},
	Pmid = {17956754},
	Rating = {0},
	Title = {On perceived synchrony-neural dynamics of audiovisual illusions and suppressions},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p8313},
