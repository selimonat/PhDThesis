	Journal = {J Neurosci},
	Language = {eng},
	Number = {21},
	Pages = {8621--8644},
	Pmid = {9334433},
	Rating = {0},
	Title = {Linearity and normalization in simple cells of the macaque primary visual cortex.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p422},
	Volume = {17},
