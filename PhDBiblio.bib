	Doi = {10.1371/journal.pbio.0040209},
	Journal = {PLoS Biol},
	Keywords = {T-Type; Cats; Geniculate Bodies; Intracellular Signaling Peptides and Proteins; Membrane Potentials; Photic Stimulation; Thalamus; Visual Pathways; Visual Perception, Animals; Calcium Channels},
	Month = {Jun},
	Number = {7},
	Pages = {e209},
	Pii = {05-PLBI-RA-1392R3},
	Pmid = {16756389},
	Rating = {0},
	Title = {Dynamic encoding of natural luminance sequences by LGN bursts.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p805},
