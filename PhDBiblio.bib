	Journal = {J Neurosci},
	Keywords = {Reproducibility of Results, Rats: Sprague-Dawley, Auditory Cortex, Membrane Potentials, Acoustic Stimulation, Animals, Rats, Models: Neurological, Signal Processing: Computer-Assisted, Patch-Clamp Techniques, Predictive Value of Tests, Neurons},
	Language = {eng},
