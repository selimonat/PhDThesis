	Pmid = {12160753},
	Rating = {0},
	Title = {Attentional modulation strength in cortical area MT depends on stimulus contrast.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p507},
	Volume = {35},
