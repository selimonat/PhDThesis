	Journal = {J Neurosci Methods},
	Language = {eng},
	Number = {1},
	Pages = {53--66},
	Pii = {S0165-0270(99)00125-9},
	Pmid = {10638815},
	Rating = {0},
	Title = {The distribution of neuronal population activation (DPA) as a tool to study interaction and integration in cortical representations.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p71},
