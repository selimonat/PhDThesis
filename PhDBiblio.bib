	Number = {7},
	Pages = {1--18},
	Rating = {0},
	Title = {On the plausibility of the discriminant center-surround hypothesis for visual saliency},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p144},
