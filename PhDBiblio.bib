	Journal = {Neuron},
	Keywords = {Photic Stimulation, Visual Cortex, Macaca mulatta, Visual Pathways, Attention, Temporal Lobe, Action Potentials, Lighting, Animals, Neurons, Motion Perception, Contrast Sensitivity, Visual Fields},
	Month = {Jul},
	Number = {2},
	Pages = {365--370},
