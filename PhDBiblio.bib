	Number = {2},
	Pages = {279--290},
	Pmid = {18372749},
	Rating = {0},
	Title = {Inhibition of return for the discrimination of faces.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p735},
	Volume = {70},
	Year = {2008},
