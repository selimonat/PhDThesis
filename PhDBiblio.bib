	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Zetzsche_Network_2005.pdf},
	Number = {2-3},
	Pages = {191--221},
	Pmid = {16411496},
	Rating = {0},
	Title = {Nonlinear and higher-order approaches to the encoding of natural scenes.},
