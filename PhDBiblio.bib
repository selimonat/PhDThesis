	Number = {10},
	Pages = {5438--5443},
	Pmid = {9144256},
	Rating = {5},
	Title = {Neural mechanisms underlying binocular fusion and stereopsis: position vs. phase.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p442},
	Volume = {94},
