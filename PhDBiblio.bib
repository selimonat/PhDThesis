	Pii = {EJN3579},
	Pmid = {15341611},
	Rating = {0},
	Title = {Directed interactions between visual areas and their role in processing image structure and expectancy.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p619},
