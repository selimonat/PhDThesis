	Rating = {0},
	Title = {Shorter latencies for motion trajectories than for flashes in population responses of cat primary visual cortex},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p987},
	Url = {http://jp.physoc.org/cgi/content/abstract/556/3/971},
	Volume = {556},
