	Pii = {00775.2003},
	Pmid = {14749316},
	Rating = {0},
	Title = {Independent components of color natural scenes resemble V1 neurons in their spatial and color tuning.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p258},
