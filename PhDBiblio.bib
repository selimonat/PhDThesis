	Keywords = {Cats, Visual Cortex, Photic Stimulation, Animals, Action Potentials, Visual Perception, Neurons, Motion Perception},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Movshon_J%20Physiol_1978.pdf},
	Month = {Oct},
	Pages = {53--77},
	Pmid = {722589},
	Rating = {0},
	Title = {Spatial summation in the receptive fields of simple cells in the cat's striate cortex.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p739},
