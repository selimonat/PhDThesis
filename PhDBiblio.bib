	Title = {Highly variable spike trains underlie reproducible sensorimotor responses in the medicinal leech.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p46},
	Volume = {22},
	Year = {2002}}

