	Number = {4},
	Pages = {482--497},
	Rating = {5},
	Read = {Yes},
	Title = {Inhibition, Spike Threshold, and Stimulus Selectivity in Primary Visual Cortex},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p344},
	Url = {http://www.sciencedirect.com/science/article/B6WSS-4RXTNW3-4/2/7fd7508538d31bdf2a495b06b3b440bc},
