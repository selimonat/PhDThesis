	Number = {11},
	Pages = {4746--4759},
	Pii = {23/11/4746},
	Pmid = {12805314},
	Rating = {0},
	Read = {Yes},
	Title = {The receptive-field organization of simple cells in primary visual cortex of ferrets under natural scene stimulation.},
