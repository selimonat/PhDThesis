	Journal = {Horm Metab Res},
	Language = {ENG},
	Pmid = {18393171},
	Rating = {0},
	Title = {Second-generation Thyrotropin Receptor Antibodies Assay and Quantitative Thyroid Scintigraphy in Autoimmune Hyperthyroidism.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p668},
