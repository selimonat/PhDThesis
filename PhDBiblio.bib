	Pmid = {17942738},
	Rating = {0},
	Title = {Audiovisual temporal correspondence modulates human multisensory superior temporal sulcus plus primary sensory cortices.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p769},
