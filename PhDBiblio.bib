	Journal = {Proc Natl Acad Sci U S A},
	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Prechtl_Proc%20Natl%20Acad%20Sci%20U%20S%20A_1997.pdf},
	Number = {14},
	Pages = {7621--7626},
	Pmid = {9207142},
	Rating = {0},
	Read = {Yes},
	Title = {Visual stimuli induce waves of electrical activity in turtle cortex.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p932},
	Volume = {94},
	Year = {1997},
