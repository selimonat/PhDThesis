	Journal = {Eur J Neurosci},
	Keywords = {Neurological; Photic Stimulation; Vision; Visual Perception, Attention; Contrast Sensitivity; Eye Movements; Humans; Models},
	Month = {Feb},
	Number = {3},
	Pages = {783--789},
	Pii = {3183},
