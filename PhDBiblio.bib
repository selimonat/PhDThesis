	Journal = {Vis Neurosci},
	Language = {eng},
	Number = {3},
	Pages = {359--363},
	Pmid = {15518214},
	Rating = {0},
	Title = {Chromatic and contrast selectivity in color contrast adaptation.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p63},
	Volume = {21},
	Year = {2004}}

