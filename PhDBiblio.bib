	Number = {11},
	Pages = {e1201},
	Pmid = {18030335},
	Rating = {0},
	Title = {The golden beauty: brain response to classical and renaissance sculptures.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p196},
	Url = {http://dx.doi.org/10.1371/journal.pone.0001201},
