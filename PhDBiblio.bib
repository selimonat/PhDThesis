	Pages = {10844--10856},
	Pii = {25/47/10844},
	Pmid = {16306397},
	Rating = {0},
	Title = {The suppressive field of neurons in lateral geniculate nucleus.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p24},
