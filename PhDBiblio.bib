	Doi = {10.1073/pnas.110135097},
	Journal = {Proc Natl Acad Sci U S A},
	Language = {eng},
	Number = {14},
	Pages = {8087--8092},
	Pii = {110135097},
	Pmid = {10869422},
	Rating = {0},
	Title = {A neuronal network model of macaque primary visual cortex (V1): orientation selectivity and dynamics in the input layer 4Calpha.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p179},
