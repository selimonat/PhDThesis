	Journal = {Eur J Neurosci},
	Keywords = {Image Cytometry, Ocular Physiology, Cats, Animals, Visual Cortex, Electrodes, Orientation, Functional Laterality, Brain Mapping, Photic Stimulation},
	Month = {Sep},
	Number = {9},
	Pages = {1973--1988},
	Pmid = {8528473},
