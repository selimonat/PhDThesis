	Keywords = {Visual; Reproducibility of Results; Saccades; Vision; Visual Perception, Algorithms; Eye Movements; Fixation, Biological; Pattern Recognition, Ocular; Humans; Models},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Renninger_Adv%20Neural%20Inf%20Process%20Syst_2005.ps},
	Pages = {1121--1128},
	Pmid = {16175670},
