	Number = {6},
	Pages = {735--753},
	Pmid = {12688669},
	Rating = {5},
	Title = {A simple model accounts for the response of disparity-tuned V1 neurons to anticorrelated images.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p284},
	Volume = {19},
