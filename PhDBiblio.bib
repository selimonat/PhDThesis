	Journal = {J Neurobiol},
	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Fr{'e}gnac_J%20Neurobiol_1999.},
	Number = {1},
	Pages = {69--82},
	Pii = {10.1002/(SICI)1097-4695(199910)41:1<69::AID-NEU10>3.0.CO;2-1},
	Pmid = {10504194},
	Rating = {4},
	Read = {Yes},
	Title = {Activity-dependent regulation of receptive field properties of cat area 17 by supervised Hebbian learning.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p98},
