	Keywords = {Animals,Antioxidants,Autoimmune Diseases,Glutathione Peroxidase,Graves Disease,Humans,Models, Biological,Neoplasms,Oxidative Stress,Selenium,Selenoprotein P,Selenoproteins,Thioredoxin-Disulfide Reductase,Thyroxine,Triiodothyronine},
