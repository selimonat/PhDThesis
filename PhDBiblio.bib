	Number = {10},
	Pages = {6.1--13},
	Pii = {/7/10/6/},
	Pmid = {17997675},
	Rating = {0},
	Title = {A bottom-up model of spatial attention predicts human error patterns in rapid scene recognition.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p559},
