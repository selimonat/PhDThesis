@article{kaschube2001a,
	Abstract = {Our visual system preferentially groups contour segments that not only have the same orientation but are colinear as well. Long-range horizontal connections are thought to play an important role in context-dependent modifications of neuronal responses. Since the topology of these connections shows a close relation to the perceptual grouping criterion of colinearity, we tested whether the statistical properties of real world images are biased towards colinear contours. By wavelet analysis we detected contours in images of natural environments and calculated their spatial correlations. In urban as well as in natural environments, the correlations between colinear contour segments were larger than the correlations for parallel contour segments. These observations indicate that colinear contour segments dominate real world images and thus might bias the functional and structural development of our visual system.},
	Author = {Matthias Kaschube and Fred Wolf and Theo Geisel and Siegrid L{\"o}wel},
