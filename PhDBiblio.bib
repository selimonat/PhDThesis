	Number = {3},
	Pages = {e481--8},
	Pii = {peds.2007-1535},
	Pmid = {18267979},
	Rating = {0},
	Title = {Predicting the likelihood of remission in children with Graves' disease: a prospective, multicenter study.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p410},
	Url = {http://dx.doi.org/10.1542/peds.2007-1535},
	Volume = {121},
