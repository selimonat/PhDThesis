	Volume = {14},
	Year = {2003}}

@article{sharma2003a,
	Abstract = {A fundamental aspect of visuomotor behavior is deciding where to look or move next. Under certain conditions, the brain constructs an internal representation of stimulus location on the basis of previous knowledge and uses it to move the eyes or to make other movements. Neuronal responses in primary visual cortex were modulated when such an internal representation was acquired: Responses to a stimulus were affected progressively by sequential presentation of the stimulus at one location but not when the location was varied randomly. Responses of individual neurons were spatially tuned for gaze direction and tracked the Bayesian probability of stimulus appearance. We propose that the representation arises in a distributed cortical network and is associated with systematic changes in response selectivity and dynamics at the earliest stages of cortical visual processing.},
	Affiliation = {Department of Brain and Cognitive Sciences, Massachusetts Institute of Technology, Cambridge, MA 02139, USA. jeetu@mit.edu},
