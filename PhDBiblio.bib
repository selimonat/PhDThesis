	Language = {eng},
	Number = {12},
	Pages = {1181--1188},
	Pmid = {10646656},
	Rating = {0},
	Title = {Radioiodine therapy in Graves' disease patients with large diffuse goiters treated with or without carbimazole at the time of radioiodine therapy.},
