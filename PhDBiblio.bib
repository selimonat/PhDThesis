	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Bosking_J%20Neurosci_1997.pdf},
	Number = {6},
	Pages = {2112--2127},
	Pmid = {9045738},
	Rating = {0},
	Read = {Yes},
	Title = {Orientation selectivity and the arrangement of horizontal connections in tree shrew striate cortex.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p333},
	Volume = {17},
