	Pages = {1948--1957},
	Pii = {bhl104},
	Pmid = {17068095},
	Rating = {0},
	Title = {Extensive overlap of mu-opioid and nicotinic sensitivity in cortical interneurons.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p845},
