	Number = {25},
	Pages = {5884--5893},
	Pii = {25/25/5884},
	Pmid = {15976077},
	Rating = {0},
	Title = {Perceptual fusion and stimulus coincidence in the cross-modal integration of speech.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p978},
	Url = {http://dx.doi.org/10.1523/JNEUROSCI.0896-05.2005},
