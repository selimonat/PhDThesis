	Number = {8},
	Pages = {e3060},
	Rating = {0},
	Title = {Estimating Receptive Fields from Responses to Natural Stimuli with Asymmetric Intensity Distributions},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p577},
