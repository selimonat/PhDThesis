	Pmid = {12037566},
	Rating = {0},
	Title = {Precise inhibition is essential for microsecond interaural time difference coding.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p942},
	Url = {http://dx.doi.org/10.1038/417543a},
