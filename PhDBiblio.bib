	Pii = {07-PLCB-RA-0104},
	Pmid = {17715996},
	Rating = {0},
	Title = {Perceptual learning via modification of cortical top-down signals.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p929},
