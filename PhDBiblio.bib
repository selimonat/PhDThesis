	Doi = {10.1016/j.cub.2004.04.028},
	Journal = {Curr Biol},
	Keywords = {Visual Fields, Motion Perception, Animals, Neurons, Attention, Male, Macaca, Visual Cortex, Psychomotor Performance, Photic Stimulation, Brain Mapping},
