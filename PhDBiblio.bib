	Pmid = {3228555},
	Rating = {0},
	Title = {Coherent oscillations: a mechanism of feature linking in the visual cortex? Multiple electrode and correlation analyses in the cat.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p383},
	Volume = {60},
