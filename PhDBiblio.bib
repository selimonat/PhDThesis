	Pii = {20/21/8111},
	Pmid = {11050133},
	Rating = {0},
	Title = {Long-term optical imaging and spectroscopy reveal mechanisms underlying the intrinsic signal and stability of cortical maps in V1 of behaving monkeys.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p465},
