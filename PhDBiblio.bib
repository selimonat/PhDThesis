	Journal = {Neuroinformatics},
	Keywords = {Neurological; Models, Acoustic Stimulation; Auditory Perception; Brain; Brain Mapping; Humans; Magnetic Resonance Imaging; Models, Theoretical; Photic Stimulation; Visual Perception},
	Number = {2},
	Pages = {93--113},
