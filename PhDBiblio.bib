	Journal = {Eur J Neurosci},
	Keywords = {Eye Movements, Contrast Sensitivity, Attention, Humans, Visual Perception, Vision, Female, Photic Stimulation, Male, Light, Adult},
	Month = {Mar},
	Number = {5},
	Pages = {1089--1097},
	Pii = {2508},
