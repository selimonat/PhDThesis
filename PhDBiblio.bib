	Keywords = {Statistical; Neurons, Neurological; Models, Afferent; Time Factors; Visual Fields; Visual Perception, Action Potentials; Algorithms; Animals; Computer Simulation; Feedback; Humans; Least-Squares Analysis; Models},
	Month = {Jun},
