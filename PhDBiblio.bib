	Pii = {MX407217P0284574},
	Pmid = {16613794},
	Rating = {0},
	Title = {Decoupling functional mechanisms of adaptive encoding.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p651},
