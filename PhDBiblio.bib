	Pmid = {18231575},
	Rating = {0},
	Title = {fMRI evidence for a cortical hierarchy of pitch pattern processing.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p1014},
