	Pii = {00645.2006},
	Pmid = {16870836},
	Rating = {0},
	Title = {Spatiotemporal patterns of an evoked network oscillation in neocortical slices: coupled local oscillators.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p198},
