	Journal = {Exp Clin Endocrinol Diabetes},
	Language = {ENG},
	Pmid = {18523918},
	Rating = {0},
	Title = {Severe Graves' Ophthalmopathy may be a Risk Factor for the Development of Postthyroidectomy Hypocalcaemia.},
