	Journal = {J Neurosci},
	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Dan_J%20Neurosci_1996.pdf},
	Number = {10},
	Pages = {3351--3362},
	Pmid = {8627371},
	Rating = {4},
	Read = {Yes},
	Title = {Efficient coding of natural scenes in the lateral geniculate nucleus: experimental test of a computational theory.},
