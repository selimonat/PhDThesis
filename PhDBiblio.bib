@article{nauhaus2008a,
	Abstract = {Summary The organization of primary visual cortex (V1) into functional maps makes individual cells operate in a variety of contexts. For instance, some neurons lie in regions of fairly homogeneous orientation preference (iso-orientation domains), while others lie in regions with a variety of preferences (e.g., pinwheel centers). We asked whether this diversity in local map structure correlates with the degree of selectivity of spike responses. We used a combination of imaging and electrophysiology to reveal that neurons in regions of homogeneous orientation preference have much sharper tuning. Moreover, in both monkeys and cats, a common principle links the structure of the orientation map, on the spatial scale of dendritic integration, to the degree of selectivity of individual cells. We conclude that neural computation is not invariant across the cortical surface. This finding must factor into future theories of receptive field wiring and map development.},
	Annote = {Excellent paper... Shows how functional differences of the neurons such as width of the tuning curves, is dependent on the position of the neuron on the cortical map. More a neuron is located in a region of functional homogeniety, more it has sharp tuning. For example a neuron within proximity of a pinwheel has very broad tuning curve. Moreover these results explains also another fact known since long time. Cats have sharper tuning curves than monkeys. Indeed the orientation maps of cats have more functionaly homogenous areas than monkeys which have really fast changing orientation maps. they state that neuron body is about 30 micrometer.},
	Author = {Ian Nauhaus and Andrea Benucci and Matteo Carandini and Dario L Ringach},
