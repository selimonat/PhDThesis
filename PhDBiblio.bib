	Journal = {J Clin Endocrinol Metab},
	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Alexander_J%20Clin%20Endocrinol%20Metab_2002.pdf},
	Number = {3},
	Pages = {1073--1077},
	Pmid = {11889166},
	Rating = {0},
	Title = {High dose of (131)I therapy for the treatment of hyperthyroidism caused by Graves' disease.},
