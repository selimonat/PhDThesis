	Number = {7},
	Pages = {1016--1029},
	Pii = {bhj044},
	Pmid = {16221931},
	Rating = {0},
	Title = {Attentional modulation of SSVEP power depends on the network tagged by the flicker frequency.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p606},
