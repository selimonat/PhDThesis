	Pmid = {9468134},
	Rating = {0},
	Title = {Collinear stimuli regulate visual responses depending on cell's contrast threshold.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p1037},
