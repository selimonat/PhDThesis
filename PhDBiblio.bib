	Journal = {J Neurosci},
	Keywords = {Motion Perception, Macaca, Photic Stimulation, Animals, Regression Analysis, Attention, Visual Cortex, Cues, Brain Mapping, Neurons, Psychomotor Performance, Temporal Lobe},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Treue_J%20Neurosci_1999.pdf},
