	Journal = {J Vis},
	Language = {eng},
	Number = {9},
	Pages = {996--1007},
	Pii = {/6/9/12/},
	Pmid = {17083291},
	Rating = {0},
	Title = {Nonlinearities in color coding: compensating color appearance for the eye's spectral sensitivity.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p67},
