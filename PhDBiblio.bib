	Doi = {10.1163/156856807782753877},
	Journal = {Spat Vis},
	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Graham_Spat%20Vis_2007.pdf},
	Number = {1-2},
	Pages = {149--164},
	Pmid = {18073056},
	Rating = {0},
	Title = {Statistical regularities of art images and natural scenes: spectra, sparseness and nonlinearities.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p571},
