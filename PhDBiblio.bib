	Keywords = {Motion Perception, Neurons, Photic St, imulation, Visual Cortex, Animals, Macaca, Visual Pathways},
	Month = {Jan},
	Number = {2},
	Pages = {366--370},
	Pii = {27/2/366},
	Pmid = {17215397},
	Rating = {0},
	Title = {Motion integration by neurons in macaque MT is local, not global.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p719},
