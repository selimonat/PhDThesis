	Number = {1},
	Pages = {49--65},
	Pmid = {17353866},
	Rating = {0},
	Title = {Neuropsychiatric aspects of hypothyroidism and treatment reversibility. .},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p479},
