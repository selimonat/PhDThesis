	Journal = {S Afr Med J},
	Language = {eng},
	Number = {7},
	Pages = {529--531},
	Pmid = {12939927},
	Rating = {0},
	Title = {Weight gain in patients after therapy for hyperthyroidism.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p649},
	Volume = {93},
	Year = {2003}}

