	Number = {6},
	Pages = {3463--3478},
	Pii = {00794.2004},
	Pmid = {15703234},
	Rating = {0},
	Title = {Neural sensitivity to interaural envelope delays in the inferior colliculus of the guinea pig.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p431},
