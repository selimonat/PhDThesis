	Keywords = {GABA-A, Video; Recepto; Receptors, N-Methyl-D-Aspartate; Signal Transduction; Somatosensory Cortex; Synapses; Tetrodotoxin; Thalamic Nuclei; rs, 2-Amino-5-phosphonovalerate; Animals; Bicuculline; Electric Stimulation; Evoked Potentials; Male; Mice; Microscopy},
	Month = {Feb},
