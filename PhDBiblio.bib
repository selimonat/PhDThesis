	Pii = {S1053811903005998},
	Pmid = {14980575},
	Rating = {0},
	Title = {Spatial and temporal factors during processing of audiovisual speech: a PET study.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p1044},
