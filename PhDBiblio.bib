	Language = {eng},
	Number = {5},
	Pages = {1698--1709},
	Pii = {21/5/1698},
	Pmid = {11222659},
	Rating = {0},
	Title = {Contextual modulation in primary visual cortex of macaques.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p137},
