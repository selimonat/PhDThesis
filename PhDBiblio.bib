	Journal = {J Neurophysiol},
	Keywords = {Action Potentials; Animals; Attention; Contrast Sensitivity; Evoked Potentials, Visual; Macaca mulatta; Male; Neurons; Parietal Lobe; Photic Stimulation; Psychomotor Performance; Saccades},
	Month = {Mar},
