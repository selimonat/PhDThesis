	Keywords = {Animals; Brain Mapping; Cats; Image Processing, Computer-Assisted; Orientation; Visual Cortex; Visual Perception},
	Month = {Oct},
	Number = {6343},
	Pages = {429--431},
	Pmid = {1896085},
	Rating = {0},
	Title = {Iso-orientation domains in cat visual cortex are arranged in pinwheel-like patterns.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p912},
