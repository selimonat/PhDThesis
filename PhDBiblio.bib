	Pii = {27/15/4120},
	Pmid = {17428990},
	Rating = {0},
	Title = {Early cross-modal interactions in auditory and visual cortex underlie a sound-induced visual illusion},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p8316},
