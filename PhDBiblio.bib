	Journal = {J Neurophysiol},
	Keywords = {Sprague-Dawley; Receptors, Muscarinic; Receptors, N-Methyl-D-Aspartate, Statistical; Electrophysiology; Evoked Potentials; Excitatory Amino Acid Antagonists; Female; Male; Membrane Potentials; Microelectrodes; Muscarinic Antagonists; Neocortex; Nerve Net; Rats; Rats, GABA-A; Receptors, Animals; Coloring Agents; Data Interpretation},
	Month = {Nov},
