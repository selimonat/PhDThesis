	Journal = {Neuron},
	Keywords = {Action Potentials; Animals; Brain; Efferent Pathways; Ganglia, Invertebrate; Insects; Neurons; Photic Stimulation; Signal Transduction; Synaptic Transmission; Visual Fields; Visual Pathways; Visual Perception},
