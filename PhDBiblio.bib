	Number = {22},
	Pages = {9517--9528},
	Pmid = {9801388},
	Rating = {0},
	Read = {Yes},
	Title = {Synaptic integration in striate cortical simple cells.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p16},
