	Language = {eng},
	Number = {2},
	Pages = {153--156},
	Pii = {S0003-4266(08)00051-6},
	Pmid = {18417090},
	Rating = {0},
	Title = {New immunomodulators in the treatment of Graves' ophthalmopathy.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p710},
	Url = {http://dx.doi.org/10.1016/j.ando.2008.02.013},
	Volume = {69},
	Year = {2008},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.ando.2008.02.013}}

