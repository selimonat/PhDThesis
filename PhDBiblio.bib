	Pages = {311--321},
	Pii = {S0896-6273(08)00572-2},
	Pmid = {18667158},
	Rating = {0},
	Title = {A presynaptic gain control mechanism fine-tunes olfactory behavior.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p231},
	Url = {http://dx.doi.org/10.1016/j.neuron.2008.07.003},
	Volume = {59},
