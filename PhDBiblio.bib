	Language = {eng},
	Number = {26},
	Pages = {6187--6198},
	Pii = {25/26/6187},
	Pmid = {15987948},
	Rating = {0},
	Title = {GABAergic inhibition controls neural gain in inferior colliculus neurons sensitive to interaural time differences.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p225},
