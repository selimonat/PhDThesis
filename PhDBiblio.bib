@article{spors2002a,
	Abstract = {We explored the spatio-temporal dynamics of odor-evoked activity in the rat and mouse main olfactory bulb (MOB) using voltage-sensitive dye imaging (VSDI) with a new probe. The high temporal resolution of VSDI revealed odor-specific sequences of glomerular activation. Increasing odor concentrations reduced response latencies, increased response amplitudes, and recruited new glomerular units. However, the sequence of glomerular activation was maintained. Furthermore, we found distributed MOB activity locked to the nasal respiration cycle. The spatial distribution of its amplitude and phase was heterogeneous and changed by sensory input in an odor-specific manner. Our data show that in the mammalian olfactory bulb, odor identity and concentration are represented by spatio-temporal patterns, rather than spatial patterns alone.},
	Affiliation = {mann Institute of Science, 76100, Rehovot, Israel. spors@mpimf-heidelberg.mpg.de},
	Author = {Hartwig Spors and Amiram Grinvald},
