	Pages = {310--317},
	Pii = {S1364-6613(99)01360-1},
	Pmid = {10431185},
	Rating = {0},
	Title = {Speechreading: illusion or window into pattern recognition.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p848},
