	Pmid = {17522315},
	Rating = {0},
	Title = {A dynamic nonlinearity and spatial phase specificity in macaque V1 neurons.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p1010},
