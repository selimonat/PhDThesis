	Pages = {69--75},
	Pmid = {11838733},
	Rating = {0},
	Title = {Smoking as a risk factor for Graves' disease, toxic nodular goiter, and autoimmune hypothyroidism.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p519},
