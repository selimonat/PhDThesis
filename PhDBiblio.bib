	Doi = {11739450},
	Journal = {The Journal of Clinical Endocrinology and Metabolism},
	Keywords = {Autonomic Nervous System,Biological Availability,Brain,Choice Behavior,Eating,Food,Graves Disease,Hormones,Humans,Osmolar Concentration,Prodrugs,Propranolol,Reference Values,Serotonin,Thyroid Hormones},
	Note = {PMID: 11739450},
