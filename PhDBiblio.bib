@article{elstrott2008a,
	Abstract = {Direction selectivity in the retina requires the asymmetric wiring of inhibitory inputs onto four subtypes of On-Off direction-selective ganglion cells (DSGCs), each preferring motion in one of four cardinal directions. The primary model for the development of direction selectivity is that patterned activity plays an instructive role. Here, we use a unique, large-scale multielectrode array to demonstrate that DSGCs are present at eye opening, in mice that have been reared in darkness and in mice that lack cholinergic retinal waves. These data suggest that direction selectivity in the retina is established largely independent of patterned activity and is therefore likely to emerge as a result of complex molecular interactions.},
	Affiliation = {Neuroscience Graduate Program, University of California, San Diego, La Jolla, CA 92093, USA. jelstrott@gmail.com},
