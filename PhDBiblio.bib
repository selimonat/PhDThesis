	Number = {7},
	Pages = {6.1--14},
	Pii = {/7/7/6/},
	Pmid = {17685802},
	Rating = {0},
	Title = {Feature-specific interactions in salience from combined feature contrasts: evidence for a bottom-up saliency map in V1.},
	Uri = {papers://CC7219F2-5773-4AFA-817A-92E1FEB5324C/Paper/p197},
