	Journal = {J Neurophysiol},
	Language = {eng},
	Local-Url = {file://localhost/Users/sonat/Documents/Papers/Anzai_J%20Neurophysiol_1999.pdf},
	Number = {2},
	Pages = {909--924},
	Pmid = {10444686},
	Rating = {5},
	Title = {Neural mechanisms for processing binocular information II. Complex cells.},
